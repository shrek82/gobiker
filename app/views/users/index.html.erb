<h1>Listing users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th>Email</th>
    <th>Reg date</th>
    <th>Login date</th>
    <th>Avatar path</th>
    <th>Point</th>
    <th>Memo</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.password %></td>
    <td><%= user.email %></td>
    <td><%= user.reg_date %></td>
    <td><%= user.login_date %></td>
    <td><%= user.avatar_path %></td>
    <td><%= user.point %></td>
    <td><%= user.memo %></td>
  </tr>
<% end %>
</table>

<br />

